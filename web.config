    <httpProtocol>
      <customHeaders>
        <remove name="Content-Security-Policy" />
        <add name="Content-Security-Policy" value="default-src 'self'; object-src 'none'; img-src 'self' data:;" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>
